{
  "images": {
    "directory": "data/images/leopardImages",
    "file_extension": ".JPG"
  },
  "ImageFilter": {
    "directory": "data/filtered_images",
    "file_extension": ".PNG"
  },
  "Mask": {
    "directory": "data/masks/leopardImages",
    "file_extension": ".BMP",
    "mrcnn": {
      "device": "/cpu:0",
      "model_directory": "data/logs",
      "weights_path": "data/logs/bottle20200221T0110/mask_rcnn_bottle_0010.h5",
      "validation_set": "recognition/mask_rcnn/samples/snow_leopard/dataset"
    }
  },
  "Keypoints": {
    "directory": "data/keypoints/leopardImages",
    "file_extension": ".PKL"
  },
  "grouping": {
    "metadata": {
      "threshold": 5
    },
	"number_of_representatives": 2  
	},
  "matching":{
  	"threshold": 5,
  	"write_matches": true
  },
  "results": {
    "directory": "data/results/leopardImages",
    "catID_csv": "data/results/catID.csv",
    "move": false
  },
  "logs": {
    "directory": "data/logs"
  }
